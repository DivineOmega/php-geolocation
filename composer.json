{
    "name": "divineomega/php-geolocation",
    "description": "PHP library that determines the country of an IP address",
    "type": "library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Jordan Hall",
            "email": "jordan@hall05.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "divineomega/php-countries": "^3.0",
        "psr/cache": "^1.0|^2.0|^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "DivineOmega\\Geolocation\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DivineOmega\\Geolocation\\Tests\\": "./tests/Unit"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
